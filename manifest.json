{
    "name": "SkipHKIS",
    "version": "1.0",
    "manifest_version": 3,
    "description": "Skip the redirect prompt",
    "background": {
        "serivce_worker": "background.js"
    },
    "permissions": [
        "storage"
    ],
    "action": {
        "default_popup": "popup.html"
    },
    "icons": {
        "16": "icons/skip16.png",
        "32": "icons/skip32.png",
        "48": "icons/skip48.png",
        "128": "icons/skip128.png"
    },
    "content_scripts":[{
        "run_at": "document_start",
        "js": ["skip.js"],
        "matches": ["*://schoology.hkis.edu.hk/*"]
    }]
}